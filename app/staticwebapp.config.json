{
  "routing": {
    "routes": [
      {
        "route": "/*}",
        "rewrite": "/index.html"
      }
    ]
  },
  "routes": [
    {
      "route": "/admin",
      "methods": [
        "get",
        "head",
        "post",
        "put",
        "delete",
        "connect",
        "options",
        "trace",
        "patch"
      ],
      "rewrite": "/app",
      "statusCode": 404
    }
  ]
}