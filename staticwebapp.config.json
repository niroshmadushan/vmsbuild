{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/login",
      "rewrite": "/index.html"
    },
    {
      "route": "/admin",
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/assets/*"]
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000",
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff"
  }
}
